grammar org.xtext.example.mydsl.MyDsl with org.eclipse.xtext.common.Terminals

generate myDsl "http://www.xtext.org/example/mydsl/MyDsl"

Model:
	commands+=Command*;
	
	
	
Command:
	Create | Load | Print | Assignment;

Create:
	'CREATE' name=ID ;

Load:
	'LOAD' name=ID ;
	
Print:
	'PRINT' name=ID ;
	
Assignment:
	symbol=ID '=' expression=Expression;


