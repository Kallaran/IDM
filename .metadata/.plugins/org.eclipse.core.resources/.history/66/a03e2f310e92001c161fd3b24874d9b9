grammar org.xtext.example.mydsl.MyDsl with org.eclipse.xtext.common.Terminals

generate myDsl "http://www.xtext.org/example/mydsl/MyDsl"

Model:
	commands+=Command*;
	
	
	
Command:
	Create | Load | Print;

Create:
	'CREATE' name=ID '=' (path+=Path)*;

Load:
	'LOAD' name=ID '=' (path+=Path)* ;
	
Print:
	'PRINT' name=ID '=' (path+=Path)* ;
	
Path:
	name=STRING;


