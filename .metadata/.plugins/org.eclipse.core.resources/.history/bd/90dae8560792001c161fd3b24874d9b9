grammar org.xtext.example.mydsl.MyDsl with org.eclipse.xtext.common.Terminals

generate myDsl "http://www.xtext.org/example/mydsl/MyDsl"

Model:
	commands+=Command*;
	
	
	
Command:
	Create | Load | Print | Store ;

Create:
	'CREATE' name=ID ;

Load:
	'LOAD' path=STRING ;
	
Print:
	'PRINT' name=ID ;
	
Store:
	id=ID '=' command=Command;


